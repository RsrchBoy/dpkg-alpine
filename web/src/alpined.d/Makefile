	  f=`echo "$$p" | sed 's,^.*/,,;s/$(EXEEXT)$$//;$(transform);s/$$/$(EXEEXT)/'`; \
	  echo " rm -f '$(DESTDIR)$(locbindir)/$$f'"; \
	  rm -f "$(DESTDIR)$(locbindir)/$$f"; \
	done

