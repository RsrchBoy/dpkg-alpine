install-loclibLTLIBRARIES: $(loclib_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(loclibdir)" || $(mkdir_p) "$(DESTDIR)$(loclibdir)"
	@list='$(loclib_LTLIBRARIES)'; for p in $$list; do \
	  if test -f $$p; then \
	    f=$(am__strip_dir) \
	    echo " $(LIBTOOL) --mode=install $(loclibLTLIBRARIES_INSTALL) $(INSTALL_STRIP_FLAG) '$$p' '$(DESTDIR)$(loclibdir)/$$f'"; \
	    $(LIBTOOL) --mode=install $(loclibLTLIBRARIES_INSTALL) $(INSTALL_STRIP_FLAG) "$$p" "$(DESTDIR)$(loclibdir)/$$f"; \
	  else :; fi; \
	done

uninstall-loclibLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@set -x; list='$(loclib_LTLIBRARIES)'; for p in $$list; do \
	  p=$(am__strip_dir) \
	  echo " $(LIBTOOL) --mode=uninstall rm -f '$(DESTDIR)$(loclibdir)/$$p'"; \
	  $(LIBTOOL) --mode=uninstall rm -f "$(DESTDIR)$(loclibdir)/$$p"; \
	done

clean-loclibLTLIBRARIES:
	-test -z "$(loclib_LTLIBRARIES)" || rm -f $(loclib_LTLIBRARIES)
	@list='$(loclib_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
